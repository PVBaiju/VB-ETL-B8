{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ADF-VBB8-ETL-DEV"
		},
		"LS_AZADLS_SFTP_SINK_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_AZADLS_SFTP_SINK'"
		},
		"LS_AZSQLDB_METADATA_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'LS_AZSQLDB_METADATA'"
		},
		"LS_SFTP_SINK_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'LS_SFTP_SINK'"
		},
		"LS_AZADLS_SFTP_SINK_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adlssftpsinkdev.dfs.core.windows.net"
		},
		"LS_AZBLOB_SINK_PRQT_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "@{linkedService().adls_storage_acc_loc}"
		},
		"LS_AZBLOB_SRV_CSV_properties_typeProperties_serviceEndpoint": {
			"type": "string",
			"defaultValue": "@{linkedService().storage_acc_loc}"
		},
		"LS_AZSQLDB_METADATA_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "sql-srvr-vb-dev.database.windows.net"
		},
		"LS_AZSQLDB_METADATA_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "sql-db-vb-dev"
		},
		"LS_AZSQLDB_METADATA_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "access-data-dev"
		},
		"LS_SFTP_SINK_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "adlssftpsinkdev.blob.core.windows.net"
		},
		"LS_SFTP_SINK_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "adlssftpsinkdev.sftpsink"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ADF-VBB8-ETL-DEV_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_AZADLS_SFTP_SINK_accountKey": {
						"value": "[parameters('LS_AZADLS_SFTP_SINK_accountKey')]"
					},
					"LS_AZSQLDB_METADATA_password": {
						"value": "[parameters('LS_AZSQLDB_METADATA_password')]"
					},
					"LS_SFTP_SINK_password": {
						"value": "[parameters('LS_SFTP_SINK_password')]"
					},
					"LS_AZADLS_SFTP_SINK_properties_typeProperties_url": {
						"value": "[parameters('LS_AZADLS_SFTP_SINK_properties_typeProperties_url')]"
					},
					"LS_AZBLOB_SINK_PRQT_properties_typeProperties_url": {
						"value": "[parameters('LS_AZBLOB_SINK_PRQT_properties_typeProperties_url')]"
					},
					"LS_AZBLOB_SRV_CSV_properties_typeProperties_serviceEndpoint": {
						"value": "[parameters('LS_AZBLOB_SRV_CSV_properties_typeProperties_serviceEndpoint')]"
					},
					"LS_AZSQLDB_METADATA_properties_typeProperties_server": {
						"value": "[parameters('LS_AZSQLDB_METADATA_properties_typeProperties_server')]"
					},
					"LS_AZSQLDB_METADATA_properties_typeProperties_database": {
						"value": "[parameters('LS_AZSQLDB_METADATA_properties_typeProperties_database')]"
					},
					"LS_AZSQLDB_METADATA_properties_typeProperties_userName": {
						"value": "[parameters('LS_AZSQLDB_METADATA_properties_typeProperties_userName')]"
					},
					"LS_SFTP_SINK_properties_typeProperties_host": {
						"value": "[parameters('LS_SFTP_SINK_properties_typeProperties_host')]"
					},
					"LS_SFTP_SINK_properties_typeProperties_userName": {
						"value": "[parameters('LS_SFTP_SINK_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}