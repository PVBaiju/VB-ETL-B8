				"annotations": [],
				"type": "AzureBlobStorage",
				"typeProperties": {
					"serviceEndpoint": "[parameters('LS_AZBLOB_SRV_CSV_properties_typeProperties_serviceEndpoint')]",
					"accountKind": "BlobStorage"
