		"LS_AZBLOB_BULK_SRV_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_AZBLOB_BULK_SRV'"
		},
		"LS_AZBLOB_CHID_JSONSINK_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_AZBLOB_CHID_JSONSINK'"
		},
		"LS_AZBLOB_CHID_JSONSRV_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_AZBLOB_CHID_JSONSRV'"
