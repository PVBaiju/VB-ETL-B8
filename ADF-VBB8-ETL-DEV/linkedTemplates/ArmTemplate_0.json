			"defaultValue": "access-data-dev"
		},
		"LS_SFTP_SINK_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "adlssftpsinkdev.blob.core.windows.net"
		},
		"LS_SFTP_SINK_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "adlssftpsinkdev.sftpsink"
