				"linkedServiceName": {
					"referenceName": "LS_AZBLOB_CHID_SINK",
					"type": "LinkedServiceReference"
				},
				"parameters": {
					"sinkcsv": {
						"type": "string"
					}
				},
				"annotations": [],
				"type": "DelimitedText",
				"typeProperties": {
					"location": {
						"type": "AzureBlobStorageLocation",
						"fileName": {
							"value": "@dataset().sinkcsv",
							"type": "Expression"
						},
						"container": "csv"
					},
					"columnDelimiter": ",",
					"escapeChar": "\\",
					"firstRowAsHeader": true,
					"quoteChar": "\""
				},
				"schema": []
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/LS_AZBLOB_CHID_SINK')]"
