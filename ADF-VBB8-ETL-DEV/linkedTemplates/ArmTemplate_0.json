			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureBlobStorage",
				"typeProperties": {
					"connectionString": "[parameters('LS_AZBLOB_CHILD_connectionString')]"
				}
			},
			"dependsOn": []
