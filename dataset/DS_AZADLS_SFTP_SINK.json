{
	"name": "DS_AZADLS_SFTP_SINK",
	"properties": {
		"linkedServiceName": {
			"referenceName": "LS_AZADLS_VB_SINK",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"filename": {
				"type": "string"
			},
			"folder": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "Binary",
		"typeProperties": {
			"location": {
				"type": "AzureBlobFSLocation",
				"fileName": {
					"value": "@concat(replace(dataset().filename,'.csv',''),'_',formatDateTime(utcNow(),'yyyy-MM-dd HH:mm:ss'),'.csv')",
					"type": "Expression"
				},
				"folderPath": {
					"value": "@dataset().folder",
					"type": "Expression"
				},
				"fileSystem": "output"
			}
		}
	}
}